---
layout: null
---
{
    {% for article in site.menus.header %}
      "{{ article.identifier | slugify }}": {
        "title": "{{ article.title | xml_escape }}",
        "id": "{{ article.identifier | xml_escape }}",
        "url": "{{ article.url | xml_escape }}",
        "category": "{{ article.parent | xml_escape }}"
      }
      {% unless forloop.last %},{% endunless %}
    {% endfor %}
}
